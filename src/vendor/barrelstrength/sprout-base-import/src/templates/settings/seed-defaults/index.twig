{% import "_includes/forms" as forms %}

{% set fieldImporters = craft.sproutImport.getSproutImportFieldImporters() %}

{% for importer in fieldImporters %}

    {% if importer.getSeedSettingsHtml() %}
        <h6>{{ importer.getName() }}</h6>

        {% namespace 'seedSettings' %}
            {{ importer.getSeedSettingsHtml()|raw }}
        {% endnamespace %}
        <hr>
    {% endif %}

{% endfor %}



