{% import "_includes/forms" as forms %}

{% set id = id ?? name %}
{% set fieldClass = fieldClass ?? name %}
{% set class = class ?? name %}

{{ forms.selectField({
    label: label,
    id: id,
    fieldClass: fieldClass,
    class: class,
    options: options,
    name: name ~ '[' ~ inputName ~ ']',
    value: value
}) }}
