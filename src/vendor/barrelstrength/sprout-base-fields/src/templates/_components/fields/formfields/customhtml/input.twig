<div id="sproutform-exampleinput-customhtml">

    <div class="customhtml-icon">{{ svg('@sproutbaseicons/code.svg') }}</div>
    <p>{{ "This Custom HTML field will only display on your front-end form."|t('sprout-forms') }}</p>

</div>

{% css %}
    #fields-sproutform-exampleinput-customhtml {
    display: flex;
    flex-direction: row;
    }
    #fields-sproutform-exampleinput-customhtml .customhtml-icon svg {
    height:48px;
    width:48px;
    fill: #ccc;
    }
    #fields-sproutform-exampleinput-customhtml p {
    flex-grow: 1;
    margin-left: 20px;
    font-style: italic;
    }
{% endcss %}
